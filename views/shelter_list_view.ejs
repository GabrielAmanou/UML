<%- include("partial_header.ejs"); %>


    
<h1>List of Shelters</h1>

<table class="table table-striped table-bordered table-hover">
    <tr>
        <td>ID</td>
        <td>Shelter Grade</td>
        <td>Shelter Equipment</td>
        <td>Maximum number of pets in shelter</td>
        <td>Shelter Size</td>
        <td>Shelter Location</td>
    </tr>
    
<% for (var c of shelter) { %>
    <tr>
        <td><%= c.shelter_id %></td>
        <td><%= c.shelter_grade %></td>
        <td><%= c.shelter_equipment %></td>
        <td><%= c.shelter_nbr_max_of_pets %></td>
        <td><%= c.shelter_size %></td>
        <td><%= c.shelter_location %></td>
    </tr>
<% } %>


</table>
<%- include("partial_footer.ejs"); %>
