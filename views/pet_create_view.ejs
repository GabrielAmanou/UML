<%- include("partial_header2.ejs"); %>



<div class="midcol">
  <div class="bloc">
    <p>Here you can add a pet to the database!</p>
    <form id="petCreate" action="/Admin/pet/finishadd" method="post">
      Owner's ID: <input type="text" name="pet_owner_id"><br><br>
      Pet's Name: <input type="text" name="pet_name"><br><br>
      Pet's Specie: <input type="text" name="pet_specie"><br><br>
      Pet's Age: <input type="text" name="pet_age"><br><br>
      Pet's Diet: <input type="text" name="pet_diet"><br><br>
      <input type="submit" onclick=""FUNCTION CREATE PET"" value="Submit">
    </form>
    <button style="background-color: rgb(0,0,0); width: 20%; position: center;" onclick="history.back()">Go Back</button>
  </div>
</div>

