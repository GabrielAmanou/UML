<%- include("partial_header2.ejs"); %>

<div class="midcol">
    <div class="bloc">
    
<h1>List of the people working for the shelters</h1>

<table class="table table-striped table-bordered table-hover">
    <tr>
        <td style="font-weight: bolder">ID</td>
        <td style="font-weight: bolder">Name</td>
        <td style="font-weight: bolder">Email</td>
        <td style="font-weight: bolder">Working Hours</td>
        <td style="font-weight: bolder">Task</td>
        <td style="font-weight: bolder">Shelter</td>
        <td style="font-weight: bolder">Password</td>
    </tr>
    <tr></tr>
    
<% for (var s of staff) { %>
    <tr>
        <td><%= s.staff_id %></td>
        <td><%= s.staff_name %></td>
        <td><%= s.staff_email %></td>
        <td><%= s.staff_working_hours %></td>
        <td><%= s.staff_task %></td>
        <td><%= s.shelter_location %></td>
        <td><%= s.staff_password %></td>
    </tr>
<% } %>

</table> <br><br>
<a href="/Admin/staff"><button style="background-color: rgb(0,0,0); width: 20%; position: center;">Go Back</button></a>
</div>
</div>


